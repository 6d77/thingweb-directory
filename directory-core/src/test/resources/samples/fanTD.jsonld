{
	"@context": [
		"https://w3c.github.io/wot/w3c-wot-td-context.jsonld",
		{
			"geo": "http://www.w3.org/2003/01/geo/wgs84_pos#",
			"saref": "http://uri.etsi.org/m2m/saref#",
			"Sensor": "saref:Sensor",
			"Temperature": "saref:Temperature",
			"Toggle": "saref:ToggleCommand",
			"unit": "saref:isMeasuredIn",
			"celsius": "http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius"
		}
	],
	"id": "urn:Fan",
	"@type": ["Thing", "Sensor"],
	"name": "Fan",
	"base": "coap:///www.example.com:5686/Fan",
	"properties": {
		"myThreshold": {
			"@type": "Temperature",
			"type": "number",
			"unit": "celsius",
			"writable": true,
			"geo:location": "room_4",
			"forms": [{
				"href": "threshold",
				"mediaType": "application/json"
			}]
		}
	},
	"actions": {
		"fanOnOff": {
			"@type": "Toggle",
			"name": "fanOnOff",
			"input": {
				"@type": "OnOff",
				"type": "boolean"
			},
			"forms": [{
				"href": "toggle",
				"mediaType": "application/json"
			}]
		},
		"fanOnOffTempAction": {
			"@type": "Toggle",
			"name": "fanOnOffTempAction",
			"input": {
				"@type": "Temperature",
				"type": "number",
				"unit": "celsius"
			},
			"geo:location": "room_4",
			"forms": [{
				"href": "fanOnOffTempAction",
				"mediaType": "application/json"
			}]
		}
	}
}